{
  "name": "client",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --check './src/**/*.{js,jsx,ts,tsx}'",
    "fix:lint": "next lint --fix",
    "fix:eslint": "eslint . --fix",
    "fix:prettier": "prettier --write .",
    "codegen": "graphql-codegen --config src/graphql/codegen.yml",
    "prepare": "husky install",
    "test": "jest --watch",
    "test-total-coverage": "jest --collectCoverage=true --coverageReporters=json-summary"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@auth0/auth0-react": "^1.9.0",
    "clsx": "^1.1.1",
    "daisyui": "^1.25.4",
    "graphql": "^16.3.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "^3.1.4",
    "recoil": "^0.6.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/node": "17.0.15",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import-access": "1.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tailwindcss": "^3.4.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.3",
    "msw": "^0.38.1",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.19",
    "typescript": "4.5.5"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
